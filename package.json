{
	"name": "devtinder-monorepo",
	"private": true,
	"version": "1.0.0",
	"workspaces": [
		"apps/backend",
		"apps/frontend"
	],
	"scripts": {
		"dev": "npm-run-all -p dev:backend dev:frontend",
		"dev:backend": "npm --prefix apps/backend run dev",
		"dev:frontend": "npm --prefix apps/frontend run dev",
		"start": "npm-run-all -p start:backend start:frontend",
		"start:backend": "npm --prefix apps/backend start",
		"start:frontend": "npm --prefix apps/frontend start",
		"build": "npm-run-all build:backend build:frontend",
		"build:backend": "echo 'nothing to build for backend'",
		"build:frontend": "npm --prefix apps/frontend run build"
	},
	"devDependencies": {
		"npm-run-all": "^4.1.5"
	}
}
